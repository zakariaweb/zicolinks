{
      
    "version": 2,

    
    "rewrites": [
          {
            "source": "/readme",
            "destination": "/readme.html"
          }
        ],
      
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=600" }
        ]
      }
    ]
  }